<h2>Latest Blocks</h2>
{{ numBlocks }}
<table>
	<thead><tr><th>Block</th><th>Approx. Time</th><th>Age</th><th>TXs</th><th>Output</th><th>Difficulty</th><th>Total Coins</th><th>%CoinDD</th></tr></thead>
	<tbody>
		{{# blocks }}
			<tr>
				<td>{{{ height }}}</td>
				<td>{{ time }}</td>
				<td>{{ age }}</td>
				<td>{{ num_tx }}</td>
				<td>{{ output }}</td>
				<td>{{ difficulty }}</td>
				<td>{{ total_coins }}</td>
				<td>{{ pct_days_destroyed }}</td>
			</tr>
		{{/ blocks }}
	</tbody>
</table>
{{> paginator }}
